{
  "name": "shopify-nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev:next": "node --require reflect-metadata ./node_modules/.bin/next dev",
    "build:next": "next build",
    "start:next": "node --require reflect-metadata ./node_modules/.bin/next start",
    "dev:server": "env-cmd tsc-watch -p tsconfig.server.json --onSuccess \"node --require reflect-metadata --require tsconfig-paths/register --enable-source-maps dist/server/src/server/server.js\"",
    "build:server": "tsc -p tsconfig.server.json",
    "start:server": "node --enable-source-maps --require reflect-metadata --require tsconfig-paths/register --optimize_for_size --max_old_space_size=2000 dist/server/src/server/server.js",
    "lint": "next lint",
    "commit": "lint-staged && cz",
    "ngrok": "ngrok start --all --config ngrok.yml"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@prisma/client": "5.15.0",
    "@sentry/nextjs": "^8",
    "@shopify/app-bridge-react": "^4.1.3",
    "@shopify/polaris": "^13.5.0",
    "@shopify/polaris-icons": "^9.1.1",
    "@tanstack/react-query": "^5.45.1",
    "@tanstack/react-query-devtools": "^5.45.1",
    "@tinymce/tinymce-react": "^5.1.1",
    "@trpc/client": "^11.0.0-rc.403",
    "@trpc/next": "^11.0.0-rc.403",
    "@trpc/react-query": "^11.0.0-rc.403",
    "@trpc/server": "^11.0.0-rc.403",
    "@vis.gl/react-google-maps": "^1.1.0",
    "got": "^12.6.1",
    "http-terminator": "^3.2.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "next": "14.2.4",
    "next-connect": "^1.0.0",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "prisma": "^5.15.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.52.0",
    "react-modal": "^3.16.1",
    "react-responsive": "^10.0.0",
    "react-toastify": "^10.0.5",
    "reflect-metadata": "^0.2.2",
    "superjson": "^2.2.1",
    "ts-custom-error": "^3.3.1",
    "tsyringe": "^4.8.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4",
    "@types/luxon": "^3",
    "@types/node": "20.14.4",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-modal": "^3",
    "@types/uuid": "^10",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "ts-essentials": "^10.0.1",
    "tsc-watch": "^6.2.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.4.5"
  },
  "packageManager": "yarn@4.3.0"
}
